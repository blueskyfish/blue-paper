    "paper-bureau": {
      "root": "apps/paper-bureau",
      "sourceRoot": "apps/paper-bureau/src",
      "projectType": "application",
      "prefix": "paper-bureau",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/paper-bureau",
            "main": "apps/paper-bureau/src/main.ts",
            "tsConfig": "apps/paper-bureau/tsconfig.app.json",
            "assets": ["apps/paper-bureau/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/paper-bureau/src/environments/environment.ts",
                  "with": "apps/paper-bureau/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "paper-bureau:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
