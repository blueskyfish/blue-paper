{
  "version": 1,
  "projects": {
    "background": {
      "root": "apps/background",
      "sourceRoot": "apps/background/src",
      "projectType": "application",
      "prefix": "background",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/background",
            "main": "apps/background/src/main.ts",
            "tsConfig": "apps/background/tsconfig.app.json",
            "assets": ["apps/background/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/background/src/environments/environment.ts",
                  "with": "apps/background/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "background:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/background/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/background/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "server-commons": {
      "root": "libs/server-commons",
      "sourceRoot": "libs/server-commons/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/server-commons/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/server-commons/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/server-commons",
            "tsConfig": "libs/server-commons/tsconfig.lib.json",
            "packageJson": "libs/server-commons/package.json",
            "main": "libs/server-commons/src/index.ts",
            "assets": ["libs/server-commons/*.md"]
          }
        }
      }
    },
    "server-database": {
      "root": "libs/server-database",
      "sourceRoot": "libs/server-database/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/server-database/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/server-database/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/server-database",
            "tsConfig": "libs/server-database/tsconfig.lib.json",
            "packageJson": "libs/server-database/package.json",
            "main": "libs/server-database/src/index.ts",
            "assets": ["libs/server-database/*.md"]
          }
        }
      }
    },
    "shared-commons": {
      "root": "libs/shared-commons",
      "sourceRoot": "libs/shared-commons/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared-commons/**/*.ts"]
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/shared-commons",
            "tsConfig": "libs/shared-commons/tsconfig.lib.json",
            "packageJson": "libs/shared-commons/package.json",
            "main": "libs/shared-commons/src/index.ts",
            "assets": ["libs/shared-commons/*.md"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/shared-commons/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "background"
}
