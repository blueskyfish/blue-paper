{
  "npmScope": "blue-paper",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "background": {
      "tags": ["backend:app"]
    },
    "server-commons": {
      "tags": ["backend:lib"]
    },
    "server-database": {
      "tags": ["backend:lib"]
    },
    "shared-commons": {
      "tags": ["commons:lib"]
    }
  }
}
