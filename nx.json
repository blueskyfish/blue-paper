{
  "npmScope": "blue-paper",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "projects": {
    "paper-bureau": {
      "tags": ["backend:app"]
    },
    "paper-kiosk": {
      "tags": ["backend:app"]
    },
    "server-commons": {
      "tags": ["backend:lib"]
    },
    "server-database": {
      "tags": ["backend:lib"]
    },
    "server-repository": {
      "tags": ["backend:lib"]
    },
    "server-editor-service": {
      "tags": ["backend:lib"]
    },
    "server-paper-service": {
      "tags": ["backend:lib"]
    },
    "server-image-commons": {
      "tags": ["backend:lib", "image:lib"]
    },
    "server-authentication": {
      "tags": ["backend:lib"]
    },
    "server-image-editor": {
      "tags": ["backend:lib", "image:lib"]
    },
    "server-image-delivery": {
      "tags": ["backend:lib", "image:lib"]
    },
    "shared-commons": {
      "tags": ["commons:lib"]
    },
    "shared-entities": {
      "tags": ["commons:lib"]
    },
    "paper-editor": {
      "tags": ["frontend:app"]
    },
    "ui-commons": {
      "tags": ["frontend:lib"]
    },
    "ui-components": {
      "tags": ["frontend:lib"]
    },
    "ui-store-editor": {
      "tags": ["frontend:lib"]
    },
    "ui-editor-backend": {
      "tags": ["frontend:lib"]
    }
  }
}
