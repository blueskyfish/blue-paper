{
  "name": "blue-paper",
  "version": "0.0.0",
  "description": "A website content manage system",
  "license": "MIT",
  "author": {
    "name": "Blueskyfish",
    "email": "blueskyfish@blueskyfish.de",
    "url": "https://blueskyfish.de"
  },
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "start:kiosk": "env-cmd -f ./blue-paper.env nx serve paper-kiosk",
    "start:bureau": "env-cmd -f ./blue-paper.env nx serve paper-bureau",
    "start:libs": "npm-watch watch:libs",
    "start:theme": "npm-run-all -p theme:*",
    "start:db": "docker-compose up -d",
    "build": "nx build",
    "build:db": "docker-compose build",
    "watch:libs": "nx run-many --target=build --projects=shared-commons,shared-entities,server-commons,server-database,server-authentication,server-repository,server-image-commons,server-image-delivery,server-image-editor,server-markdown,server-paper-service --parallel=true",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "stop:db": "docker-compose down",
    "theme:http": "http-server ./themes/hall/src/ -p 28090",
    "theme:sass": "sass ./themes/hall/scss/styles.scss ./themes/hall/src/assets/styles.css --watch -I ./node_modules"
  },
  "watch": {
    "watch:libs": "./libs/**/*.*"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.3",
    "bootstrap": "^4.5.3",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "marked": "^1.2.2",
    "moment": "^2.29.1",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "nunjucks": "^3.2.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.5",
    "sharp": "^0.26.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@nrwl/cli": "10.3.0",
    "@nrwl/eslint-plugin-nx": "10.3.0",
    "@nrwl/jest": "10.3.0",
    "@nrwl/nest": "10.3.0",
    "@nrwl/node": "10.3.0",
    "@nrwl/workspace": "10.3.0",
    "@types/express": "^4.17.8",
    "@types/jest": "26.0.8",
    "@types/marked": "^1.1.0",
    "@types/multer": "^1.4.4",
    "@types/mysql": "^2.15.15",
    "@types/node": "~12.19.1",
    "@types/nunjucks": "^3.1.3",
    "@types/sharp": "^0.26.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "dotenv": "6.2.0",
    "env-cmd": "^10.1.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "http-server": "^0.12.3",
    "jest": "26.2.2",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.7.0",
    "prettier": "2.0.4",
    "sass": "^1.27.0",
    "ts-jest": "26.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.0.0",
    "typescript": "~4.0.3"
  }
}
